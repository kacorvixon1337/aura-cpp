    ImVec2 clip_off = draw_data->DisplayPos;         
    ImVec2 clip_scale = draw_data->FramebufferScale; 

    
    for (int n = 0; n < draw_data->CmdListsCount; n++)
    {
        const ImDrawList* cmd_list = draw_data->CmdLists[n];
        const ImDrawVert* vtx_buffer = cmd_list->VtxBuffer.Data;
        const ImDrawIdx* idx_buffer = cmd_list->IdxBuffer.Data;
        glVertexPointer(2, 0x1406, sizeof(ImDrawVert), (const GLvoid*)((const char*)vtx_buffer + __builtin_offsetof(ImDrawVert,pos)));
        glTexCoordPointer(2, 0x1406, sizeof(ImDrawVert), (const GLvoid*)((const char*)vtx_buffer + __builtin_offsetof(ImDrawVert,uv)));
        glColorPointer(4, 0x1401, sizeof(ImDrawVert), (const GLvoid*)((const char*)vtx_buffer + __builtin_offsetof(ImDrawVert,col)));

        for (int cmd_i = 0; cmd_i < cmd_list->CmdBuffer.Size; cmd_i++)
        {
            const ImDrawCmd* pcmd = &cmd_list->CmdBuffer[cmd_i];
            if (pcmd->UserCallback)
            {
                
                
                if (pcmd->UserCallback == (ImDrawCallback)(-1))
                    ImGui_ImplOpenGL2_SetupRenderState(draw_data, fb_width, fb_height);
                else
                    pcmd->UserCallback(cmd_list, pcmd);
            }
            else
            {
                
                ImVec2 clip_min((pcmd->ClipRect.x - clip_off.x) * clip_scale.x, (pcmd->ClipRect.y - clip_off.y) * clip_scale.y);
                ImVec2 clip_max((pcmd->ClipRect.z - clip_off.x) * clip_scale.x, (pcmd->ClipRect.w - clip_off.y) * clip_scale.y);
                if (clip_max.x <= clip_min.x || clip_max.y <= clip_min.y)
                    continue;

                
                glScissor((int)clip_min.x, (int)((float)fb_height - clip_max.y), (int)(clip_max.x - clip_min.x), (int)(clip_max.y - clip_min.y));

                
                glBindTexture(0x0DE1, (GLuint)(intptr_t)pcmd->GetTexID());
                glDrawElements(0x0004, (GLsizei)pcmd->ElemCount, sizeof(ImDrawIdx) == 2 ? 0x1403 : 0x1405, idx_buffer + pcmd->IdxOffset);
            }
        }
    }

    
    glDisableClientState(0x8076);
    glDisableClientState(0x8078);
    glDisableClientState(0x8074);
    glBindTexture(0x0DE1, (GLuint)last_texture);
    glMatrixMode(0x1700);
    glPopMatrix();
    glMatrixMode(0x1701);
    glPopMatrix();
    glPopAttrib();
    glPolygonMode(0x0404, (GLenum)last_polygon_mode[0]); glPolygonMode(0x0405, (GLenum)last_polygon_mode[1]);
    glViewport(last_viewport[0], last_viewport[1], (GLsizei)last_viewport[2], (GLsizei)last_viewport[3]);
    glScissor(last_scissor_box[0], last_scissor_box[1], (GLsizei)last_scissor_box[2], (GLsizei)last_scissor_box[3]);
    glShadeModel(last_shade_model);
    glTexEnvi(0x2300, 0x2200, last_tex_env_mode);
}

bool ImGui_ImplOpenGL2_CreateFontsTexture()
{
    
    ImGuiIO& io = ImGui::GetIO();
    ImGui_ImplOpenGL2_Data* bd = ImGui_ImplOpenGL2_GetBackendData();
    unsigned char* pixels;
    int width, height;
    io.Fonts->GetTexDataAsRGBA32(&pixels, &width, &height);   

    
    
    GLint last_texture;
    glGetIntegerv(0x8069, &last_texture);
    glGenTextures(1, &bd->FontTexture);
    glBindTexture(0x0DE1, bd->FontTexture);
    glTexParameteri(0x0DE1, 0x2801, 0x2601);
    glTexParameteri(0x0DE1, 0x2800, 0x2601);
    glPixelStorei(0x0CF2, 0);
    glTexImage2D(0x0DE1, 0, 0x1908, width, height, 0, 0x1908, 0x1401, pixels);

    
    io.Fonts->SetTexID((ImTextureID)(intptr_t)bd->FontTexture);

    
    glBindTexture(0x0DE1, last_texture);

    return true;
}

void ImGui_ImplOpenGL2_DestroyFontsTexture()
{
    ImGuiIO& io = ImGui::GetIO();
    ImGui_ImplOpenGL2_Data* bd = ImGui_ImplOpenGL2_GetBackendData();
    if (bd->FontTexture)
    {
        glDeleteTextures(1, &bd->FontTexture);
        io.Fonts->SetTexID(0);
        bd->FontTexture = 0;
    }
}

bool    ImGui_ImplOpenGL2_CreateDeviceObjects()
{
    return ImGui_ImplOpenGL2_CreateFontsTexture();
}

void    ImGui_ImplOpenGL2_DestroyDeviceObjects()
{
    ImGui_ImplOpenGL2_DestroyFontsTexture();
}
