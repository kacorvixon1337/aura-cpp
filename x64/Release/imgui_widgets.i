bool ImGui::Bind(const char* label, int* key, const ImVec2& size_arg)
{
    ImGuiWindow* window = ImGui::GetCurrentWindow();
    if (window->SkipItems)
        return false;

    ImGuiContext& g = *GImGui;
    ImGuiIO& io = g.IO;
    const ImGuiStyle& style = g.Style;

    const ImGuiID id = window->GetID(label);
    const ImVec2 label_size = ImGui::CalcTextSize(label, 0, true);
    ImVec2 size = ImGui::CalcItemSize(ImVec2(45, 20), ImGui::CalcItemWidth(), label_size.y + style.FramePadding.y * 2.0f);
    
    

    const ImRect frame_bb(window->DC.CursorPos, window->DC.CursorPos + size);
    const ImRect total_bb(frame_bb.Min, frame_bb.Max + ImVec2(label_size.x > 0.0f ? style.ItemInnerSpacing.x + label_size.x : 0.0f, 0.0f));

    ImGui::ItemSize(total_bb, style.FramePadding.y);
    if (!ImGui::ItemAdd(total_bb, id))
        return false;

    const bool focus_requested = ImGui::FocusableItemRegister(window, g.ActiveId == id);
    const bool focus_requested_by_code = focus_requested && (window->FocusIdxAllCounter == window->FocusIdxAllRequestCurrent);
    const bool focus_requested_by_tab = focus_requested && !focus_requested_by_code;

    const bool hovered = ImGui::ItemHoverable(frame_bb, id);

    if (hovered) {
        ImGui::SetHoveredID(id);
        g.MouseCursor = ImGuiMouseCursor_TextInput;
    }

    const bool user_clicked = hovered && io.MouseClicked[0];

    if (focus_requested || user_clicked) {
        if (g.ActiveId != id) {
            
            memset(io.MouseDown, 0, sizeof(io.MouseDown));
            memset(io.KeysDown, 0, sizeof(io.KeysDown));
            *key = 0;
        }
        ImGui::SetActiveID(id, window);
        ImGui::FocusWindow(window);
    }
    else if (io.MouseClicked[0]) {
        
        if (g.ActiveId == id)
            ImGui::ClearActiveID();
    }

    bool value_changed = false;
    int bind = *key;

    if (g.ActiveId == id) {
        if (!value_changed) {
            for (auto i = 1; i < 256; i++) {
                if (io.KeysDown[i]) {
                    bind = i;
                    value_changed = true;
                    ImGui::ClearActiveID();
                }
            }
        }

        if (IsKeyPressedMap(ImGuiKey_Escape)) {
            *key = 0;
            ClearActiveID();
        }
        else {
            *key = bind;
        }
    }

    
    

    char buf_display[64] = "None";

    if (*key != 0 && g.ActiveId != id) {
        strcpy_s(buf_display, KeyNames[*key]);
    }
    else if (g.ActiveId == id) {
        strcpy_s(buf_display, "...");
    }
    static float alpha = 1.f;
    if (g.ActiveId == id)
    {
        alpha -= 0.06f;
        if (alpha < 0.4f)
            alpha = 0.4f;
    }
    else
    {
        alpha += 0.02f;
        if (alpha > 1.f)
            alpha = 1.f;
    }
    const ImRect clip_rect(frame_bb.Min.x, frame_bb.Min.y, frame_bb.Min.x + size.x, frame_bb.Min.y + size.y); 
    ImVec2 render_pos = frame_bb.Min + style.FramePadding;
    
    PushStyleColor(ImGuiCol_Text, GetColorU32(ImGuiCol_Text, alpha));
    RenderTextClipped(total_bb.Min + style.FramePadding, frame_bb.Max - style.FramePadding, buf_display, 0, 0, style.ButtonTextAlign, &clip_rect);
    PopStyleColor();
    if (label_size.x > 0)
        RenderText(ImVec2(frame_bb.Max.x + style.ItemInnerSpacing.x, frame_bb.Min.y + style.FramePadding.y + 30), label);

    return value_changed;
}
