        static const char* fmt_table_float[3][4] =
        {
            {   "%0.3f",   "%0.3f",   "%0.3f",   "%0.3f" }, 
            { "R:%0.3f", "G:%0.3f", "B:%0.3f", "A:%0.3f" }, 
