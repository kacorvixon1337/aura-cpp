        {
            if (f[1] == 0)
                f[0] = g.ColorEditLastHue;
            if (f[2] == 0)
                f[1] = g.ColorEditLastSat;
        }
    }
    int i[4] = { ((int)((f[0]) * 255.0f + ((f[0])>=0 ? 0.5f : -0.5f))), ((int)((f[1]) * 255.0f + ((f[1])>=0 ? 0.5f : -0.5f))), ((int)((f[2]) * 255.0f + ((f[2])>=0 ? 0.5f : -0.5f))), ((int)((f[3]) * 255.0f + ((f[3])>=0 ? 0.5f : -0.5f))) };

    bool value_changed = false;
